{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [
      "node"
    ],
    "paths": {
      "@abp/*":["../node_modules/abp-ng2-module/dist/src/*"],
      "@abp/":["../node_modules/abp-ng2-module/dist/src/"],
      "@shared": ["./shared"],
      "@shared/*": ["./shared/*"],
      "@core": ["app/core"],
      "@core/*": ["app/core/*"],
      "@testing": ["testing"],
      "@testing/*": ["testing/*"],
      "@env": ["environments"],
      "@env/*": ["environments/*"]
    },
    "baseUrl": "./"
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}